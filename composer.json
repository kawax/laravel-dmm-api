{
  "name": "revolution/laravel-dmm-api",
  "description": "DMM API for Laravel",
  "keywords": [
    "dmm",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3||^8.0",
    "illuminate/support": "*",
    "dmmcomlabo/dmm-sdk-v3": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Dmm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Dmm\\Providers\\DmmServiceProvider"
      ],
      "aliases": {
        "Dmm": "Revolution\\Dmm\\Facades\\Dmm"
      }
    }
  }
}
